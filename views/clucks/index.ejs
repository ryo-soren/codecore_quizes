<%- include('../partials/header') %>
    <% for( let cluck of clucks) { %>
    <div class="card m-auto d-flex w-75 mb-3">
        <div class="card-body d-flex">
            <div class="avatar-container mx-2">
                <img src="https://i.pinimg.com/736x/b8/67/43/b86743ae04db856b95a964afd9db4fb0--pet-quotes-bird-watching.jpg" class="img w-100"alt="<%-cluck.team_name%>">
            </div>
            <div class="m-auto">
                <h5 class="text-secondary">
                    <%-cluck.username%>
                </h5>
                <p class="card-text text-secondary"><%-cluck.content%></p>
                    <img src="<%-cluck.image_url ? cluck.image_url : "images/image.png"%>" class="img-fluid" alt="<%-cluck.team_name%>">
            </div>
        </div>
    </div>
    <% } %>

<%- include('../partials/footer') %>
